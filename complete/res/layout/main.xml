<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:id="@+id/main_act"
              android:orientation="vertical"
              android:layout_width="fill_parent"
              android:layout_height="fill_parent">
  <TextView android:id="@+id/stack"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text=" "
            android:gravity="right"/>
  <EditText android:id="@+id/field"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text=" "
            android:gravity="right"
            android:enabled="false" />
  <GridLayout android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:layout_gravity="center"
              android:columnCount="8">
    <Button android:id="@+id/sto"
            android:text="&gt;x"
            android:layout_row="0"
            android:layout_column="0"
            android:layout_width="50dip"
            android:layout_height="50dip"
            android:onClick="onClick" />
    <Button android:id="@+id/rtrv"
            android:text="&lt;x"
            android:layout_row="1"
            android:layout_column="0"
            android:layout_width="50dip"
            android:layout_height="50dip"
            android:onClick="onClick" />
    <Button android:id="@+id/pop"
            android:text="pop"
            android:layout_row="2"
            android:layout_column="0"
            android:layout_width="50dip"
            android:layout_height="50dip"
            android:onClick="onClick" />
    <Button android:id="@+id/swap"
            android:text="swp"
            android:layout_row="3"
            android:layout_column="0"
            android:layout_width="50dip"
            android:layout_height="50dip"
            android:onClick="onClick" />
    <Button android:id="@+id/back"
            android:text="C"
            android:layout_row="0"
            android:layout_column="4"
            android:layout_width="50dip"
            android:layout_height="50dip"
            android:onClick="onClick" />
    <Button android:id="@+id/_7"
            android:text="7"
            android:layout_row="0"
            android:layout_column="0"
            android:layout_width="50dip"
            android:layout_height="50dip"
            android:onClick="onClick" />
    <Button android:id="@+id/_8"
            android:text="8"
            android:layout_row="0"
            android:layout_column="1"
            android:layout_width="50dip"
            android:layout_height="50dip"
            android:onClick="onClick" />
    <Button android:id="@+id/_9"
            android:text="9"
            android:layout_row="0"
            android:layout_column="2"
            android:layout_width="50dip"
            android:layout_height="50dip"
            android:onClick="onClick" />
    <Button android:id="@+id/sin"
            android:text="sin"
            android:layout_row="0"
            android:layout_column="3"
            android:layout_width="50dip"
            android:layout_height="50dip"
            android:onClick="onClick" />
    <Button android:id="@+id/div"
            android:text="/"
            android:layout_row="1"
            android:layout_column="4"
            android:layout_width="50dip"
            android:layout_height="50dip"
            android:onClick="onClick" />
    <Button android:id="@+id/_4"
            android:text="4"
            android:layout_row="1"
            android:layout_column="0"
            android:layout_width="50dip"
            android:layout_height="50dip"
            android:onClick="onClick" />
    <Button android:id="@+id/_5"
            android:text="5"
            android:layout_row="1"
            android:layout_column="1"
            android:layout_width="50dip"
            android:layout_height="50dip"
            android:onClick="onClick" />
    <Button android:id="@+id/_6"
            android:text="6"
            android:layout_row="1"
            android:layout_column="2"
            android:layout_width="50dip"
            android:layout_height="50dip"
            android:onClick="onClick" />
    <Button android:id="@+id/cos"
            android:text="cos"
            android:layout_row="1"
            android:layout_column="3"
            android:layout_width="50dip"
            android:layout_height="50dip"
            android:onClick="onClick" />
    <Button android:id="@+id/mul"
            android:text="*"
            android:layout_row="1"
            android:layout_column="4"
            android:layout_width="50dip"
            android:layout_height="50dip"
            android:onClick="onClick" />
    <Button android:id="@+id/_1"
            android:text="1"
            android:layout_row="2"
            android:layout_column="0"
            android:layout_width="50dip"
            android:layout_height="50dip"
            android:onClick="onClick" />
    <Button android:id="@+id/_2"
            android:text="2"
            android:layout_row="2"
            android:layout_column="1"
            android:layout_width="50dip"
            android:layout_height="50dip"
            android:onClick="onClick" />
    <Button android:id="@+id/_3"
            android:text="3"
            android:layout_row="2"
            android:layout_column="2"
            android:layout_width="50dip"
            android:layout_height="50dip"
            android:onClick="onClick" />
    <Button android:id="@+id/tan"
            android:text="tan"
            android:layout_row="2"
            android:layout_column="3"
            android:layout_width="50dip"
            android:layout_height="50dip"
            android:onClick="onClick" />
    <Button android:id="@+id/sub"
            android:text="-"
            android:layout_row="2"
            android:layout_column="4"
            android:layout_width="50dip"
            android:layout_height="50dip"
            android:onClick="onClick" />
    <Button android:id="@+id/_0"
            android:text="0"
            android:layout_row="3"
            android:layout_column="0"
            android:layout_width="50dip"
            android:layout_height="50dip"
            android:onClick="onClick" />
    <Button android:id="@+id/dec"
            android:text="."
            android:layout_row="3"
            android:layout_column="1"
            android:layout_width="50dip"
            android:layout_height="50dip"
            android:onClick="onClick" />
    <Button android:id="@+id/neg"
            android:text="(-)"
            android:layout_row="3"
            android:layout_column="2"
            android:layout_width="50dip"
            android:layout_height="50dip"
            android:onClick="onClick" />
    <Button android:id="@+id/eval"
            android:text="="
            android:layout_row="3"
            android:layout_column="3"
            android:layout_width="50dip"
            android:layout_height="50dip"
            android:onClick="onClick" />
    <Button android:id="@+id/add"
            android:text="+"
            android:layout_row="3"
            android:layout_column="4"
            android:layout_width="50dip"
            android:layout_height="50dip"
            android:onClick="onClick" />
    <Button android:id="@+id/pi"
            android:text="Pi"
            android:layout_row="4"
            android:layout_column="0"
            android:layout_width="50dip"
            android:layout_height="50dip"
            android:onClick="onClick" />
  </GridLayout>
</LinearLayout>
